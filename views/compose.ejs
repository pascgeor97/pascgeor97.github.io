<%- include('./partials/header'); -%>

    <h1 class="card-header">Compose</h1>

    <form class="" action="/compose" method="post">

        <div class="form-group">

            <label>Title</label>
            <input class="form-control" type="text" name="newPublishTitle" autocomplete="off">
            <label>Post</label>
            <textarea class="form-control" type="text" name="newPublishPost" autocomplete="off" rows="5" cols="30"></textarea>
            

